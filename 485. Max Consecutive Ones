class Solution:
    def findMaxConsecutiveOnes(self, nums: List[int]) -> int:
        max_count = 0
        current_count = 0
        
        for num in nums:
            if(num == 1):
                current_count += 1
            else:
                current_count = 0
            max_count = max(current_count, max_count)
        
        return max_count
